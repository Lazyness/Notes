<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="guru.work.prog.swing.NoteForm">
  <grid id="27dc6" binding="createNotePanel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <autoscrolls value="true"/>
      <background color="-11644075"/>
      <foreground color="-11644075"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="8860e" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="0.0"/>
        </constraints>
        <properties>
          <font name="Algerian" size="18" style="2"/>
          <foreground color="-1052689"/>
          <text value="Create your Note"/>
        </properties>
      </component>
      <scrollpane id="b6e07">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="3" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties>
          <background color="-11644075"/>
          <font name="Algerian" size="14" style="2"/>
        </properties>
        <border type="bevel-lowered" title="Title">
          <title-color color="-1052689"/>
        </border>
        <children>
          <component id="403df" class="javax.swing.JTextPane" binding="textPaneTitle">
            <constraints/>
            <properties>
              <background color="-11644075"/>
              <font name="DialogInput" size="16" style="2"/>
              <foreground color="-1052689"/>
              <name value=""/>
            </properties>
          </component>
        </children>
      </scrollpane>
      <scrollpane id="2ca28">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="3" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="10.0"/>
        </constraints>
        <properties>
          <background color="-11644075"/>
          <font name="Algerian" size="14" style="2"/>
          <foreground color="-11644075"/>
        </properties>
        <border type="bevel-lowered" title="Write your note...">
          <title-color color="-1052689"/>
        </border>
        <children>
          <component id="36a26" class="javax.swing.JTextPane" binding="textText">
            <constraints/>
            <properties>
              <background color="-11644075"/>
              <font name="DialogInput" size="16" style="2"/>
              <foreground color="-1052689"/>
              <name value=""/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </scrollpane>
      <component id="dae3f" class="javax.swing.JButton" binding="finishButton" default-binding="true">
        <constraints>
          <grid row="3" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Finish"/>
        </properties>
      </component>
      <component id="1a3eb" class="javax.swing.JLabel">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
